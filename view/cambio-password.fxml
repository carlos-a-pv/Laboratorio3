<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="245.0" prefWidth="586.0" style="-fx-background-color: #F0E7B5;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="laboratorio3.controller.CambiarPasswordController">
   <children>
      <Label layoutX="140.0" layoutY="7.0" text="Cambio de contraseña" textFill="#141412">
         <font>
            <Font name="SansSerif Bold" size="27.0" />
         </font>
      </Label>
      <Label layoutX="69.0" layoutY="72.0" text="Nueva contraseña">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="69.0" layoutY="124.0" text="Repita la contraseña">
         <font>
            <Font name="SansSerif Regular" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnConfirmar" layoutX="178.0" layoutY="181.0" mnemonicParsing="false" onAction="#onClickConfirmar" style="-fx-background-color: #000000;" text="Confirmar" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnCancelar" layoutX="313.0" layoutY="181.0" mnemonicParsing="false" onAction="#onClickCancelar" prefHeight="39.0" prefWidth="110.0" style="-fx-background-color: #000000;" text="Cancelar" textFill="WHITE">
         <font>
            <Font name="SansSerif Bold" size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="tfNuevaPassword" layoutX="266.0" layoutY="70.0" prefHeight="32.0" prefWidth="274.0" />
      <PasswordField fx:id="tfPasswordRepetida" layoutX="266.0" layoutY="119.0" prefHeight="32.0" prefWidth="274.0" />
   </children>
</AnchorPane>
